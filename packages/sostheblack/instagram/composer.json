{
    "name": "sostheblack/instagram",
    "description": "Instagram PHP Scraper. Get account information, photos and videos without any authorization",
    "keywords": [
        "instagram",
        "scraper"
    ],
    "type": "library",
    "homepage": "https://github.com/sostheblack/instagram",
    "license": "MIT",
    "authors": [
        {
            "name": "raiym",
            "email": "r.kapishev@gmail.com"
        },
        {
            "name": "SOSTheBlack",
            "email": "jeancesargarcia@gmail.com"
        }
    ],
    "require": {
        "php": ">=8",
        "ext-curl": "*",
        "ext-json": "*",
        "psr/simple-cache": "~1.0",
        "psr/http-client": "~1.0",
        "guzzlehttp/psr7": "^2"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0",
        "phpfastcache/phpfastcache": "^7.1",
        "guzzlehttp/guzzle": "^7.2"
    },
    "autoload": {
        "psr-4": {
            "InstagramScraper\\": "src/InstagramScraper/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "InstagramScraper\\Tests\\": "tests/"
        }
    },
    "support": {
        "email": "jeancesargarcia@gmail.com"
    }
}
